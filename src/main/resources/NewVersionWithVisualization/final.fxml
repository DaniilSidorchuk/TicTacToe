<GridPane alignment="CENTER" hgap="15" prefHeight="500" prefWidth="500" vgap="15" xmlns="http://javafx.com/javafx/8.0.76-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="NewVersionWithVisualization.FinalLog">
   <children>
      <Label text="GAME OVER" GridPane.columnIndex="0" GridPane.columnSpan="4" GridPane.halignment="CENTER" GridPane.rowIndex="0">
      <font>
          <Font name="System Bold" size="48.0" />
      </font>
      </Label>
      <Label fx:id="typeOfFinal" GridPane.columnIndex="0" GridPane.columnSpan="4" GridPane.halignment="CENTER" GridPane.rowIndex="5">
          <font>
              <Font name="System Bold" size="18.0" />
          </font>
      </Label>
      <Label fx:id="winnerLabel" GridPane.columnIndex="0" GridPane.columnSpan="4" GridPane.halignment="CENTER" GridPane.rowIndex="7">
          <font>
              <Font name="System Bold" size="20.0" />
          </font>
      </Label>
      <Button fx:id="statistic"
              prefHeight="25.0" prefWidth="185.0" text="Show statistic" textFill="#f20707"
              GridPane.columnIndex="0" GridPane.columnSpan="4" GridPane.halignment="CENTER" GridPane.rowIndex="16"
      onAction="#statistic">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Label text="Made by Daniil Sidorchuk" GridPane.columnIndex="0" GridPane.columnSpan="4" GridPane.halignment="CENTER" GridPane.rowIndex="20">
          <font>
              <Font size="8.0" />
          </font>

      </Label>

